{
  "name": "glyph-components",
  "version": "1.1.0",
  "description": "Core web components for glyph-ui",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/fibersjs/fibersjs.esm.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --watch --serve",
    "start:dev": "npm-run-all --parallel start start:docs",
    "start:docs": "start-storybook --quiet -s ./www -p 6007 --no-manager-cache",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "build:docs": "build-storybook --quiet -s ./dist -o ./../../docs/webcomponents && npm run creevey",
    "creevey": "creevey --extract && move-file stories.json ./../../docs/webcomponents/stories.json",
    "lint": "eslint src/**/*{.ts,.tsx} --fix"
  },
  "dependencies": {
    "@open-wc/lit-helpers": "^0.3.12",
    "@stencil/core": "^2.0.1",
    "@storybook/addon-postcss": "^2.0.0",
    "exceljs": "^4.2.1",
    "file-saver": "^2.0.5",
    "glyph-core": "*",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "perfect-scrollbar": "^1.5.0",
    "stencil-click-outside": "^1.7.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@stencil/angular-output-target": "0.0.5",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.1.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/web-components": "^6.1.21",
    "@types/numeral": "^2.0.1",
    "css-loader": "^5.2.0",
    "lit-html": "^1.3.0",
    "move-file-cli": "^2.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass": "^1.32.8",
    "sass-loader": "10.1.1",
    "style-loader": "^2.0.0"
  }
}
